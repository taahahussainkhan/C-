#include<iostream>
//#include<time.h>
using namespace std;
int main()
{
	int arr[5] = {}, arr1[5] = {},i;
	cout << "Enter in first array: \n";
	for (i = 0; i < 5; i++)
		cin>>arr[i];
	cout << "Enter in second array: \n";
	for (i = 0; i < 5; i++)
		cin >> arr1[i];
	cout << "Elements of first array are: \n";
	for (i = 0; i < 5; i++)
		cout << arr[i] << " ";
		cout << endl;
		cout << "Elements of second array are: \n";
	for (i = 0; i < 5; i++)
		cout<< arr1[i]<<" ";
	cout << endl;
	cout << "The common elements are: \n";
	for ( i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			if (arr[i] == arr1[j])
			{
				cout << endl;
				cout << arr[i] << endl;
			}
		}
	}
	
		system("pause");
	return 0;
}
