#include<iostream>
//#include<time.h>
using namespace std;
int main()
{
	char arr[50] = {}, arr1[50] = {},i;
	cout << "Enter in first array: \n";
	//for (i = 0; i < 5; i++)
		//cin>>arr[i];
	cin.getline(arr, 50);
	cout << "Enter in second array: \n";
	//for (i = 0; i < 5; i++)
		//cin >> arr1[i];
	cin.getline(arr1, 50);
	cout << "Elements of first array are: \n";
	for (i = 0; i < 50; i++)
		cout << arr[i] ;
		cout << endl;
		cout << "Elements of second array are: \n";
	for (i = 0; i < 50; i++)
		cout<< arr1[i];
	cout << endl;
	cout << "The common elements are: \n";
	for ( i = 0; arr[i]!='\0'; i++)
	{
		for (int j = 0; arr1[j]!='\0'; j++)
		{
			if (arr[i] == arr1[j])
			{
				
				cout << arr[i] ;
			}
		}
	}
	cout << endl;
	
		system("pause");
	return 0;
}
