#include <iostream>
#include<time.h>
using namespace std;
int main()
{
	char arr[1000] = {};
	srand(time(0));
	for (int i = 0; i <= 999; i++)
	{
		arr[i] = rand() % 122;
		if (arr[i] >= 65 && arr[i] <= 90)
			cout << arr[i] << " ";
	}
	cout << endl << endl;
		for (int i = 0; i <= 999; i++)
		{
		arr[i] = rand() % 122;
		if (arr[i] >= 97 && arr[i] <= 122)
			cout << arr[i] << " ";
	}
	return 0;
}//by me
